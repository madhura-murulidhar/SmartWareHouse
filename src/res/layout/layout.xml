<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".layoutclass"
    android:background="@color/btn_login"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"

    >




    <Button
        android:id="@+id/button12"
        android:layout_width="170dp"
        android:layout_height="75dp"
        android:text="Rice"
        android:radius="10dp"
        android:background="@drawable/mybutton"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        />





    <Button
        android:id="@+id/button20"
        android:layout_width="165dp"
        android:layout_height="75dp"
        android:text="Wheat"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:background="@drawable/mybutton"

        android:layout_above="@+id/button13"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2" />


    <Button
        android:layout_width="165dp"
        android:layout_height="75dp"
        android:text="login details"
        android:id="@+id/button14"
        android:background="@drawable/mybutton"

        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:layout_below="@+id/button12"
        android:layout_alignLeft="@+id/button12"
        android:layout_alignStart="@+id/button12"
        android:layout_toLeftOf="@+id/button20"
        android:layout_toStartOf="@+id/button20" />

    <Button
        android:layout_width="165dp"
        android:layout_height="75dp"
        android:text="Database"
        android:id="@+id/button13"
        android:background="@drawable/mybutton"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:layout_above="@+id/buttonDisc"
        android:layout_toRightOf="@+id/button16"
        android:layout_toEndOf="@+id/button16" />


    <Button

        android:layout_width="165dp"
        android:layout_height="75dp"
        android:text="about"
        android:id="@+id/button16"
        android:background="@drawable/mybutton"
        android:layout_below="@+id/button14"
        android:layout_alignLeft="@+id/button14"
        android:layout_alignStart="@+id/button14"
        android:layout_toLeftOf="@+id/button20"
        android:layout_toStartOf="@+id/button20" />


    <Button

        android:layout_width="165dp"
        android:layout_height="75dp"
        android:text="Terms of Service"
        android:id="@+id/buttonDisc"
        android:background="@drawable/mybutton"
        android:layout_alignTop="@+id/button16"
        android:layout_toRightOf="@+id/button12"
        android:layout_alignRight="@+id/button20"
        android:layout_alignEnd="@+id/button20" />

    <Button

        android:layout_width="165dp"
        android:layout_height="75dp"
        android:text="Developers"
        android:id="@+id/buttonCredits"
        android:background="@drawable/mybutton"
        android:layout_below="@+id/button16"
        android:layout_alignLeft="@+id/button16"
        android:layout_alignStart="@+id/button16"
        android:layout_toLeftOf="@+id/buttonDisc"
        android:layout_toStartOf="@+id/buttonDisc" />

    <Button

        android:layout_width="165dp"
        android:layout_height="75dp"
        android:text="Open Source Libraries"
        android:id="@+id/buttonlib"
        android:background="@drawable/mybutton"
        android:layout_alignTop="@+id/buttonCredits"
        android:layout_alignRight="@+id/buttonDisc"
        android:layout_alignEnd="@+id/buttonDisc"
        android:layout_toRightOf="@+id/button16"
        android:layout_toEndOf="@+id/button16" />

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Warehouse Management"
    android:id="@+id/textView2"
    android:layout_gravity="center_horizontal"
    android:textColor="@color/white"
    android:textSize="30dp"

        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />



    <ImageView
        android:background="@drawable/wa"
        android:layout_gravity="center_horizontal"
        android:layout_width="120dp"
        android:layout_height="120dp"

        android:id="@+id/imageView2"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true" />

</RelativeLayout>